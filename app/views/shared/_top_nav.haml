          %li
            = link_to "Sign Out", destroy_admin_session_path, method: :delete
      %ul#mobile-demo.side-nav
        %li
          = link_to "About", about_index_path
        %li
          = link_to "Gallery", gallery_index_path
        %li
          = link_to "Contact", contact_index_path
        - if admin_signed_in?
          %li
            = link_to "Sign Out", destroy_admin_session_path, method: :delete
