.navbar-fixed
  %nav#top-nav{class: @nav_class}
    .nav-wrapper
      %a.brand-logo{href: '/'}
        %img.responsive-img.nav-logo.logo{src: asset_path('speed_shop_logo_long.png')}
      %a.button-collapse{"data-activates" => "mobile-demo", href: "#"}
        = fa_icon 'bars'
      %ul.right.hide-on-med-and-down
        %li
          = link_to "About", about_index_path
        %li
          = link_to "Gallery", gallery_index_path
        %li
          = link_to "Contact", contact_index_path
        - if admin_signed_in?
          %li
            = link_to "Sign Out", destroy_admin_session_path, method: :delete
        %li
          = link_to "Vendors", vendors_path
      %ul#mobile-demo.side-nav
        %li
          = link_to "About", about_index_path
        %li
          = link_to "Gallery", gallery_index_path
        %li
          = link_to "Contact", contact_index_path
        - if admin_signed_in?
          %li
            = link_to "Sign Out", destroy_admin_session_path, method: :delete
